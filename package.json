{
  "name": "@djforth/jasmine-call-helpers",
  "version": "2.0.0-beta.1",
  "description": "Jasmine Check calls helper",
  "main": "index.js",
  "repository": "https://github.com/djforth/jasmine-call-helpers.git",
  "author": "Adrian Stainforth <adrian.stainforth@gmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": "morsedigital",
    "globals": {
      "fdescribe": false,
      "describe": false,
      "beforeEach": false,
      "afterEach": false,
      "it": false,
      "expect": false,
      "spyOn": false,
      "I18n": false,
      "jasmine": false
    },
    "rules": {
      "require-jsdoc": 0
    }
  },
  "devDependencies": {
    "@djforth/ap_karma_wp": "^1.0.4",
    "@djforth/ap_utils": "^3.0.1",
    "@djforth/stubs-spy-manager": "^3.0.1-beta",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "eslint": "^3.10.2",
    "eslint-config-morsedigital": "^1.1.0",
    "eslint-plugin-jasmine": "^2.1.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "jasmine-core": "^2.5.2",
    "lodash": "^4.*.*"
  },
  "scripts": {
    "build": "babel src -x '.js' -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "asset-pipeline-karma",
    "test:mac": "asset-pipeline-karma -b",
    "test:watch": "asset-pipeline-karma -w"
  }
}
